<Page x:Class="GestureBaseUI_Project.PictureRecorder"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GestureBaseUI_Project"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PictureRecorder">
    <StackPanel>


        <Image x:Name="MyIm" Width="500"></Image>
        <Image x:Name="MyIm2" Width="500"></Image>
        <Image x:Name="MyIm1" Width="500"></Image>

        <Label Name="Text" >

        </Label>
        <Canvas x:Name="canvas" Width="500" Height="280">
            <Ellipse x:Name="eli" Height="10" Width="10" Fill="Blue" Canvas.Top="140" Canvas.Left="250" />
        </Canvas>
    </StackPanel>

</Page>
