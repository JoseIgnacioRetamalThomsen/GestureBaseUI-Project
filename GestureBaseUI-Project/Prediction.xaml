<Page x:Class="GestureBaseUI_Project.Prediction"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GestureBaseUI_Project"
      mc:Ignorable="d" 
      d:DesignHeight="800" d:DesignWidth="1200"
      Title="Prediction">

    <StackPanel Width="1200" Height="800"  HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
        <Canvas Width="1200" Height="800" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
            <Ellipse x:Name="Point" Width="10" Height="10" Canvas.Left="1200" Canvas.Top="800" Fill="Black" />
          
        </Canvas>
    </StackPanel>
</Page>
